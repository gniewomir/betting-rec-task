{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/betting/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/betting/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/betting",
  "projects": {
    "bets": {
      "type": "application",
      "root": "apps/bets",
      "entryFile": "main",
      "sourceRoot": "apps/bets/src",
      "compilerOptions": {
        "tsConfigPath": "apps/bets/tsconfig.app.json"
      }
    },
    "betting": {
      "type": "application",
      "root": "apps/betting",
      "entryFile": "main",
      "sourceRoot": "apps/betting/src",
      "compilerOptions": {
        "tsConfigPath": "apps/betting/tsconfig.app.json"
      }
    },
    "odds": {
      "type": "application",
      "root": "apps/odds",
      "entryFile": "main",
      "sourceRoot": "apps/odds/src",
      "compilerOptions": {
        "tsConfigPath": "apps/odds/tsconfig.app.json"
      }
    }
  }
}